public class PrintExample {
    public static void main(String[] args) {
        System.out.print("Hello ");
        System.out.print("World!");
      
        System.out.println();
        System.out.println("This is a new line.");
        
        int a = 5;
        String name = "Java";
        System.out.println("The value of a is: " +name);
    }}
