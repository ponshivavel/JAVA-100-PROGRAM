import java.util.*;
 class power {
    public static void main(String[] args) {
   Scanner in = new Scanner(System.in);
  System.out.println("Enter the number");
       int a = in.nextInt();
    System.out.println("Enter the power number");
       int b = in.nextInt();
       int result =(int)Math.pow(a,b);
        System.out.println(result);
    }
    }
