import java.util.*;
 class ODDOREVEN {
    public static void main(String[] args) {
   Scanner in = new Scanner(System.in);
  System.out.println("Enter the number");
       int n = in.nextInt();


       if(n/2==0){
        System.out.println("IT IS EVEN NUMBER");
     }
    else
       System.out.println("IT IS ODD NUMBER");
}}
