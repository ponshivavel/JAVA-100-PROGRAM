import java.util.*;
class hashmap{
    public static void main(String[] args) {
        
    System.out.println("ENTER THE DETIALS");
    HashMap<String,Integer>empee = new HashMap<>(); 
    Scanner in = new Scanner(System.in);
    for( int i =0;i<3;i++){
       
       String a =in.nextLine();
       int b = in.nextInt();
       in.nextLine();
       empee.put(a,b);
     
}  in.close();
        
        for(Map.Entry<String,Integer> entry :empee.entrySet()){
        System.out.println("name:"+entry.getKey()+"   emploeeid    "+entry.getValue());}
   
}
}
