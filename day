import java.util.*;
 class oddoreveninarray {
    public static void main(String[] args) {
   Scanner scan = new Scanner(System.in);
    int a[] = new int[4];
System.out.println("ENTER THE NUMBER");
   for(int i=0;i<4;i++){
        a[i] = scan.nextInt();
        
    }
    int even = 0;
    int odd = 0;
   
   for(int i=0;i<a.length;i++)
   {
    if(a[i]%2 == 0) {
         even++;
        }else {
      odd++;
    }}
System.out.println(even);
    System.out.println(odd);
}}
   
    
