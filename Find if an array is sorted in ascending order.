import java.util.*;
 class checkthesort {
    public static void main(String[] args) {
   Scanner scan = new Scanner(System.in);
    int a[] = new int[4];
    int b[] = new int[4];
System.out.println("ENTER THE NUMBER");
   for(int i=0;i<4;i++){
        a[i] = scan.nextInt();
         b[i]=a[i];
    }
    
     Arrays.sort(a);

   for(int i=0;i<a.length;i++)
   {
    if(a[i]==b[i]) {
        System.out.println("IT IS ALREADY SORTED");
        break;
        }else {  System.out.println("IT IS NOT  SORTED");
    break;
    }}

}}
   
