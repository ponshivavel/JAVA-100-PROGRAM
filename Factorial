import java.util.*;
class factorial{
  public static void main(String []arg)  {
    Scanner in = new Scanner(System.in);
System.out.println("ENTER THE NUMBER");
    int a = in.nextInt();
int f=1;
    for(int i=1;i<=a;i++){
      f*=i;
    }
  System.out.println("THE FACTORIAL NUMBER FOR"+a+"IS"+f);f);  }
}
