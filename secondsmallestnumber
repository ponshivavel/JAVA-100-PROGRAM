import java.util.*;
 class secondsmallestnumber {
    public static void main(String[] args) {
   Scanner scan = new Scanner(System.in);
    int a[] = new int[4];
System.out.println("ENTER THE NUMBER");
   for(int i=0;i<4;i++){
        a[i] = scan.nextInt();
    }
        Arrays.sort(a);
    int ssmall =Integer.MIN_VALUE;
     int larg =a[0];
    
   for(int i=1;i<a.length;i++)
   {
    if(a[i]>larg){
        ssmall=a[i];
        break;
    }}
System.out.println("THE SECOND LARGESTNUMBER IS"+ssmall);
   }}
   
    
